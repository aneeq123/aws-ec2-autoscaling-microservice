spring:
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQL8Dialect

  datasource:
    url: jdbc:mysql://<RDS-ENDPOINT>:3306/<DB-NAME>
    username: <DB-USERNAME>
    password: <DB-PASSWORD>
    driver-class-name: com.mysql.cj.jdbc.Driver

server:
  port: 9091
